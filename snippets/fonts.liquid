{% # theme-check-disable %}
{%- unless settings.type_body_font.system? -%}
  <link
    rel="preload"
    as="font"
    href="{{ settings.type_body_font | font_url }}"
    type="font/woff2"
    crossorigin
    fetchpriority="low"
  >
{%- endunless -%}
